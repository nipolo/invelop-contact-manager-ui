{
  "name": "contact-manager",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:beta": "ng serve --configuration beta",
    "build": "ng build",
    "build:beta": "ng build --configuration beta",
    "docker:build:dev": "docker build -f Dockerfile -t invelop-contact-manager-ui-local/latest .",
    "docker:build:beta": "docker build -f Dockerfile.beta -t invelop-contact-manager-ui-beta/latest .",
    "docker:run:dev": "docker run --name invelop-contact-manager-ui-local -d -p 4200:80 invelop-contact-manager-ui-local/latest",
    "docker:run:beta": "docker run --name invelop-contact-manager-ui-beta -d -p 4200:80 invelop-contact-manager-ui-beta/latest",
    "docker:rebuild:dev": "docker container stop invelop-contact-manager-ui-local && docker container rm invelop-contact-manager-ui-local && npm run docker:build:dev && npm run docker:run:dev",
    "docker:rebuild:beta": "docker container stop invelop-contact-manager-ui-beta && docker container rm invelop-contact-manager-ui-beta && npm run docker:build:beta && npm run docker:run:beta",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.3.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@ngrx/schematics": "^13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}
